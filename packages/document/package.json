{
  "name": "@acot/document",
  "version": "0.0.7",
  "description": "A set of tools for working with documentation for acot presets.",
  "homepage": "https://github.com/acot-a11y/acot/tree/main/packages/document",
  "bugs": {
    "url": "https://github.com/acot-a11y/acot/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acot-a11y/acot.git",
    "directory": "packages/document"
  },
  "license": "MIT",
  "author": "wadackel <wadackel@gmail.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "templates",
    "!__mocks__",
    "!__tests__"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:script": "tsc",
    "build:style": "cross-env NODE_ENV=production yarn postcss",
    "postcss": "postcss styles/styles.css -o templates/assets/styles.css",
    "test": "jest",
    "watch": "run-p watch:*",
    "watch:script": "yarn build:script -w",
    "watch:style": "yarn postcss -w"
  },
  "jest": {
    "preset": "ts-jest",
    "rootDir": "src",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts?(x)"
    ]
  },
  "dependencies": {
    "@acot/core": "0.0.7",
    "@acot/factory": "0.0.7",
    "@acot/find-chrome": "0.0.5",
    "@acot/html-pickup": "0.0.5",
    "@acot/logger": "0.0.5",
    "debug": "^4.3.1",
    "deepmerge": "^4.2.2",
    "fast-glob": "^3.2.2",
    "fastify": "^3.8.0",
    "fastify-static": "^3.3.0",
    "indent-string": "^4.0.0",
    "is-plain-object": "^5.0.0",
    "log-symbols": "^4.0.0",
    "mdast-util-to-string": "^2.0.0",
    "micromatch": "^4.0.2",
    "mustache": "^4.0.1",
    "plur": "^4.0.0",
    "puppeteer-core": "^5.5.0",
    "remark-parse": "^9.0.0",
    "remark-stringify": "^9.0.0",
    "strip-ansi": "^6.0.0",
    "type-fest": "^0.20.2",
    "unified": "^9.0.0",
    "unist-util-select": "^3.0.2",
    "unist-util-visit-parents": "^3.1.0"
  },
  "devDependencies": {
    "@types/micromatch": "4.0.1",
    "@types/mustache": "4.1.1",
    "@types/puppeteer-core": "5.4.0",
    "@types/unist": "2.0.3",
    "postcss": "8.2.4",
    "postcss-cli": "8.3.1",
    "postcss-import": "14.0.0",
    "postcss-nested": "5.0.3",
    "tailwindcss": "2.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
